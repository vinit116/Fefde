<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/H35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.35" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/H70"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.7" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/V10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/V90"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.9" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@id/V10"
        app:layout_constraintRight_toRightOf="@id/V90"
        app:layout_constraintTop_toTopOf="@id/H35"
        app:layout_constraintBottom_toBottomOf="@id/H70"
        android:background="@drawable/dialog_box_outside">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/H50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.5" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/H30"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.3" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/H80"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.8" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/V50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5" />

        <ImageView
            android:id="@+id/food_potli_layout"
            android:layout_width="92dp"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="@id/V50"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@id/H50"
            android:src="@drawable/potli_bg"/>

        <ImageView
            android:id="@+id/travel_potli_layout"
            android:layout_width="92dp"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="@id/V50"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@id/H50"
            android:src="@drawable/potli_bg"/>

        <ImageView
            android:id="@+id/rent_potli_layout"
            android:layout_width="92dp"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="@id/V50"
            app:layout_constraintTop_toTopOf="@id/H50"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/potli_bg"/>

        <ImageView
            android:id="@+id/other_potli_layout"
            android:layout_width="92dp"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="@id/V50"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/H50"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/potli_bg"/>

        <ImageView
            android:id="@+id/food_potli_img"
            android:layout_width="87dp"
            android:scaleX="2.5"
            android:scaleY="2.5"
            android:layout_height="41dp"
            app:layout_constraintTop_toTopOf="@id/food_potli_layout"
            android:src="@drawable/food_pot"
            app:layout_constraintLeft_toLeftOf="@id/food_potli_layout"
            app:layout_constraintRight_toRightOf="@id/food_potli_layout"
            android:layout_marginTop="8dp" />

        <ImageView
            android:id="@+id/travel_potli_img"
            android:layout_width="87dp"
            android:scaleX="1.7"
            android:scaleY="1.4"
            android:layout_height="41dp"
            android:src="@drawable/travel_pot"
            app:layout_constraintTop_toTopOf="@id/travel_potli_layout"
            app:layout_constraintLeft_toLeftOf="@id/travel_potli_layout"
            app:layout_constraintRight_toRightOf="@id/travel_potli_layout"
            android:layout_marginTop="8dp" />

        <ImageView
            android:id="@+id/rent_potli_img"
            android:layout_width="87dp"
            android:scaleX="2.5"
            android:scaleY="2.2"
            android:layout_height="41dp"
            android:src="@drawable/rent_pot"
            app:layout_constraintTop_toTopOf="@id/rent_potli_layout"
            app:layout_constraintLeft_toLeftOf="@id/rent_potli_layout"
            app:layout_constraintRight_toRightOf="@id/rent_potli_layout"
            android:layout_marginTop="12dp" />

        <ImageView
            android:id="@+id/other_potli_img"
            android:layout_width="87dp"
            android:scaleX="2"
            android:scaleY="2"
            android:layout_height="41dp"
            android:src="@drawable/others_pot"
            app:layout_constraintTop_toTopOf="@id/other_potli_layout"
            app:layout_constraintLeft_toLeftOf="@id/other_potli_layout"
            app:layout_constraintRight_toRightOf="@id/other_potli_layout"
            android:layout_marginTop="12dp" />

        <TextView
            android:id="@+id/food_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Food"
            app:layout_constraintTop_toTopOf="@id/H30"
            app:layout_constraintLeft_toLeftOf="@id/food_potli_layout"
            app:layout_constraintRight_toRightOf="@id/food_potli_layout"
            android:textColor="#1b1b18"
            android:textSize="16sp"
            android:shadowColor="#40000000"
            android:shadowDx="0"
            android:shadowDy="4"
            android:shadowRadius="4"/>

        <TextView
            android:id="@+id/travel_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Travel"
            app:layout_constraintTop_toTopOf="@id/H30"
            app:layout_constraintLeft_toLeftOf="@id/travel_potli_layout"
            app:layout_constraintRight_toRightOf="@id/travel_potli_layout"
            android:textColor="#1b1b18"
            android:textSize="16sp"
            android:shadowColor="#40000000"
            android:shadowDx="0"
            android:shadowDy="4"
            android:shadowRadius="4"/>

        <TextView
            android:id="@+id/rent_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Rent"
            app:layout_constraintTop_toTopOf="@id/H80"
            app:layout_constraintLeft_toLeftOf="@id/rent_potli_layout"
            app:layout_constraintRight_toRightOf="@id/rent_potli_layout"
            android:textColor="#1b1b18"
            android:textSize="16sp"
            android:shadowColor="#40000000"
            android:shadowDx="0"
            android:shadowDy="4"
            android:shadowRadius="4"/>

        <TextView
            android:id="@+id/others_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Others"
            app:layout_constraintTop_toTopOf="@id/H80"
            app:layout_constraintLeft_toLeftOf="@id/other_potli_layout"
            app:layout_constraintRight_toRightOf="@id/other_potli_layout"
            android:textColor="#1b1b18"
            android:textSize="16sp"
            android:shadowColor="#40000000"
            android:shadowDx="0"
            android:shadowDy="4"
            android:shadowRadius="4"/>


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>